{
    "name": "masrodjie/codeigniter4-queue",
    "description": "The Illuminate Queue package for CodeIgniter 4",
    "keywords": ["codegniter4", "codeigniter4-queue", "queue", "redis", "worker", "job"],
    "license": "MIT",
    "authors": [
        {
            "name": "masrodjie",
            "email": "arnas@arnas.web.id"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "codeigniter4/framework": "^4.1",
        "illuminate/queue": "^8.83",
        "illuminate/redis": "^8.83",
        "illuminate/events": "^8.83",
        "predis/predis": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Masrodjie\\Queue\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always -vvv"
    }
}
